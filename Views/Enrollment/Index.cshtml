@{
}



@model List<ComplexExample.Models.Enrollment>


<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css"
          integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <title></title>
</head>

@*<span class=""></span>*@
<h3 class="text-secondary float-start"> Students </h3>
<hr class="my-4" />
@*<p class="display-6 text-danger">@ViewBag.error </p>*@
<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">Course Id</th>
            <th scope="col">Enrollment Id</th>
            <th scope="col">Grade</th>
            <th scope="col">Student Id</th>
            <th colspan="2" scope="col">Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var enroll in Model)
        {

            <tr>
                <th scope="row">@enroll.CourseID</th>
                <td>@enroll.EnrollmentID</td>
                <td>@enroll.Grade</td>
                <td>@enroll.StudentID</td>
                <td>@Html.ActionLink("Update", "UpdateStudent", "Admin", new { id = enroll.EnrollmentID }) . @Html.ActionLink("Delete", "DeleteStudent", "Admin", new { id = enroll.EnrollmentID })</td>

            </tr>


        }

    </tbody>
</table>
<br />
